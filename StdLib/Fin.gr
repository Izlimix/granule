data Fin : Nat -> Type where
  FZ : forall (k : Nat) . Fin (k + 1);
  FS : forall (k : Nat) . Fin k -> Fin (k + 1)

dropFin : forall (n : Nat) . Fin n -> ()
dropFin FZ = ();
dropFin (FS k) = dropFin k
