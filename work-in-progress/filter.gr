filter : âˆ€ {a : Type, n : Nat, m : Nat} . {m <= n}
         => (a -> Bool) [n] -> Vec n (a [1..2]) -> Vec m a
filter [_] Nil = Nil;
filter [p] (Cons [x] xs) = if p x then Cons x (filter [p] xs) else filter [p] xs
